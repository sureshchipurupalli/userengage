<% if @errors.present? && @errors.size > 0  %>
$("#rowalerts").html("<%= j render 'shared/errors', errors: @errors %>");
<% else %>
$("#rowalerts").html("<%= j render 'shared/success', display_text: 'password updated successfully.' %>");
$("#user_password").val("");
$("#user_new_password").val("");
$("#user_password_confirmation").val("");
<% end %>
