%meta{charset:'utf-8'}/
%meta{content:'IE=edge,chrome=1', 'http-equiv' => 'X-UA-Compatible'}/
%meta{content:'width=device-width, initial-scale=1.0', name:'viewport'}/
%title= content_for?(:title) ? yield(:title) : 'UserEngage'
%meta{content:content_for?(:description) ? yield(:description) : '', name:'description'}/
%meta{content:content_for?(:author) ? yield(:author) : 'UserEngage team', name:'author'}/
= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true
= stylesheet_link_tag 'theme', media:'all'
= stylesheet_link_tag "https://fonts.googleapis.com/icon?family=Material+Icons"
= stylesheet_link_tag "https://storage.googleapis.com/code.getmdl.io/1.0.6/material.pink-blue.min.css"
= stylesheet_link_tag "https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.9.4/css/bootstrap-select.min.css"
= javascript_include_tag 'application', 'data-turbolinks-track' => true
= javascript_include_tag 'https://storage.googleapis.com/code.getmdl.io/1.0.6/material.min.js'
= javascript_include_tag 'https://rawgit.com/FezVrasta/bootstrap-material-design/master/dist/js/material.min.js'
= javascript_include_tag 'http://momentjs.com/downloads/moment-with-locales.min.js'
= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.9.4/js/bootstrap-select.min.js'
= javascript_include_tag 'http://www.jqueryscript.net/demo/Pretty-Date-Time-Picker-Plugin-For-Bootstrap-Material/js/bootstrap-material-datetimepicker.js'
= javascript_include_tag '//www.google.com/jsapi'
= javascript_include_tag 'http://html5shiv.googlecode.com/svn/trunk/html5.js'

= csrf_meta_tags

